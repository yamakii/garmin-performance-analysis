# Analysis Workspace Environment
# Copy to .env and customize

# MCP server code path (default: main repo)
# For worktree verification: /absolute/path/to/worktree/packages/garmin-mcp-server
GARMIN_MCP_SERVER_DIR=../packages/garmin-mcp-server

# DuckDB and raw data path (default: local fixture)
GARMIN_DATA_DIR=./data

# Report output path (default: local, not production)
GARMIN_RESULT_DIR=./result
